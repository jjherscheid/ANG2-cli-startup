{
  "name": "ang2-cli-startup",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "start:external": "ng serve --host 0.0.0.0",
    "test": "ng test --code-coverage",
    "test:single": "ng test --auto-watch false --single-run true --code-coverage",
    "debug:test" : "ng test --debug",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "showcoverage": "lite-server -c coveragebrowser.json",
    "build": "ng build --prod",
    "lint": "ng lint --format=stylish --force"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.10",
    "@angular/compiler": "2.4.10",
    "@angular/core": "2.4.10",
    "@angular/forms": "2.4.10",
    "@angular/http": "2.4.10",
    "@angular/platform-browser": "2.4.10",
    "@angular/platform-browser-dynamic": "2.4.10",
    "@angular/router": "3.4.10",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "jquery": "1.9.1 - 2",
    "rxjs": "5.2.0",
    "ts-helpers": "1.1.2",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.4",
    "@angular/compiler-cli": "2.4.10",
    "@types/jasmine": "2.5.46",
    "@types/jquery": "2.0.41",
    "@types/node": "7.0.8",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-coverage-istanbul-reporter": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-phantomjs-launcher": "1.0.4",
    "protractor": "~4.0.13",
    "ts-mocks": "0.2.2",
    "ts-node": "3.0.0",
    "tslint": "4.5.1",
    "typescript": "2.2.1"
  }
}
